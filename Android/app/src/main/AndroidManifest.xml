<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chiYo.MarketPlace">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:configChanges="keyboardHidden|orientation"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="chiYo.MarketPlace.MainActivity" />
        <activity
            android:name="chiYo.MarketPlace.CheckOutActivity"
            android:label="@string/title_activity_check_out"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="chiYo.MarketPlace.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="chiYo.MarketPlace.CompleteOrder" />
        <activity android:name="chiYo.MarketPlace.signUp"></activity>
    </application>

</manifest>
